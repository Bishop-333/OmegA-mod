models/players/tony/headred
{
	cull disable
	{
		map models/players/tony/headred.tga
		rgbGen lightingDiffuse
	}
	{
		map models/players/tony/headred.tga
		blendfunc gl_src_alpha gl_one
		rgbGen lightingDiffuse
		alphaGen lightingSpecular
		detail
	}
}

